<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE logback>
<included>
	<appender name="FILE" class="ch.qos.logback.core.FileAppender">
		<file>/tmp/camel.log</file>
		<encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%nhogehoge</pattern>
		</encoder>
	</appender>
	<root level="INFO">
		<appender-ref ref="FILE" />
	</root>
</included>
